<h1>New Book</h1>

<%= form_for @book do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :year_published %>
    <%= f.number_field :year_published %>
  </p>
  <p>
    <%= f.label :pages %>
    <%= f.number_field :pages %>
  </p>
  <p>
    <%= f.label :thumbnail %>
    <%= f.text_field :thumbnail %>
  </p>
  <p>
    Author Name(s)<input type="text" name="author_names">
  </p

  <p>
  <%= f.submit %>
  </p>
<% end %>
