<head>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<h1>brian typing</h1>
<% @books.each do |book| %>
<div class="card-bg card d-inline-flex p-2 bd-highlight index-uniformer" style="width: 18rem;">
  <img src= <%= "#{book.thumbnail}" %> class="card-img-top img" alt="...">
  <div class="card-body card-bg">
    <h5 class="card-title"> <%= book.title %> </h5>
    <p class="card-text">
    <% book.authors.each do |author| %>
    Authors: <%= link_to author.name, author_path(author) %> <br>
    <% end %>
    pages: <%= book.pages %>
    <br>
    Published: <%= book.year_published %>
    </p>
    <a href= <%= book_path(book) %> class="btn btn-primary <%= book.title %>"> See More </a>
  </div>
</div>
<% end %>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>Mark</td>
      <td>Otto</td>
      <td>@mdo</td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>Thornton</td>
      <td>@fat</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td>Larry</td>
      <td>the Bird</td>
      <td>@twitter</td>
    </tr>
  </tbody>
</table>
